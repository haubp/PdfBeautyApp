    <!-- Detail Page-->
    <string name="open_settings">Open Settings</string>
    <!--Jailbreakreak-->
    <string name="detail_jailbreak_title_good">Your device doesn\'t appear to be rooted.</string>
    <string name="detail_jailbreak_description">Rooting a device disables built-in security measures and makes it much more vulnerable to malware and information theft.</string>

    <string name="detail_jailbreak_title_bad">Your device appears to be rooted.</string>
    <string name="detail_jailbreak_title_sub_bad">Rooting a device disables built-in security measures and makes it much more vulnerable to malware and information theft.</string>
    <string name="detail_jailbreak_solution_1">1. <b>Unroot</b> your device</string>

    <!--Password-->
    <string name="detail_password_title_good">Your device is secured with a lock screen plus </string>
    <string name="detail_password_description">Without a password, the data on your device may be accessed by anyone in possession of your device. This can lead to information and identity theft.</string>

    <string name="detail_password_title_bad">Your device is not secured with a lock screen and passcode.</string>
    <string name="detail_password_title_sub_bad">Without a password, the data on your device may be accessed by anyone in possession of your device. This can lead to identity theft.</string>
    <string name="detail_password_solution_1">1. Open the <b>Settings</b> app</string>
    <string name="detail_password_solution_2">2. Go to <b>Lock screen</b> (it may be under "Security")</string>
    <string name="detail_password_solution_3">3. Select a screen lock type</string>
    <string name="detail_password_solution_4">4. Choose a pin, password, or pattern that only you know</string>

    <!--Ad tracking-->
    <string name="detail_ad_tracking_title_good">Your device has ad tracking set to limited.</string>
    <string name="detail_ad_tracking_description">Ad tracking allows advertisers to build detailed profiles about you based on your Internet browsing history. Disabling this feature improves your privacy.</string>

    <string name="detail_ad_tracking_title_bad">Your device is opted in to ad tracking.</string>
    <string name="detail_ad_tracking_title_sub_bad">Ad tracking allows advertisers to build detailed profiles about you based on your Internet browsing history. Opting out of ad tracking will improve your privacy.</string>
    <string name="detail_ad_tracking_solution">Search for instructions on how to opt out of ad tracking for your phone to opt out ad tracking and reset advertising ID (if any).</string>

    <!--Encryption-->
    <string name="detail_encryption_title_good">Your device storage is encrypted.</string>
    <string name="detail_encryption_description">Encrypting your device will prevent data from being extracted in case the device is lost or stolen.</string>
    <string name="detail_encryption_description_2">Not all encryption is equal &#8212;&#8212; longer passcodes dramatically increase resistance to brute force decryption attacks.</string>

    <string name="detail_encryption_title_bad">Your device storage doesn\'t appear to be encrypted.</string>
    <string name="detail_encryption_title_sub_bad">Encrypted devices prevent data from being extracted in case they are lost or stolen.</string>
    <string name="detail_encryption_solution">Search for instructions on how to encrypt your device to prevent data breach.</string>

    <!--Os update-->
    <string name="detail_os_title_good">Your device\'s Operating System (OS) is up-to-date.</string>
    <string name="detail_os_description">The latest OS may contain new features and is usually the most secure version.</string>

    <!--Free storage-->
    <string name="detail_free_storage_title_good">Your device has sufficient free storage.</string>
    <string name="detail_free_storage_description">This will enable you to update your apps or operating system to the latest versions when needed.</string>

    <string name="detail_free_storage_title_bad">Your device does not have sufficient free storage.</string>
    <string name="detail_free_storage_title_sub_bad">This means that you may not be able to update your apps or operating system to the latest versions. It may also cause some apps to crash.</string>
    <string name="detail_free_storage_solution_1">1. Remove large apps or media that occupy significant disk space</string>
    <string name="detail_free_storage_solution_2">2. Install or replace your external memory card (if available for your device)</string>
    <string name="used">Used: \u0020</string>
    <string name="available">Available: \u0020</string>

    <!--Available memory-->
    <string name="detail_memory_title_good">Your device has sufficient free memory.</string>
    <string name="detail_memory_description">Running with less than 50 MB of available memory may cause slowness or instability.</string>

    <string name="detail_memory_title_bad">Your device has insufficient free memory.</string>
    <string name="detail_memory_title_sub_bad">Running with less than 50 MB of available memory may cause slowness or instability.\nThe operating system should free up memory for you. Otherwise you can:</string>
    <string name="detail_memory_solution_1">1. Upgrade to a newer device with more memory</string>
    <string name="detail_memory_solution_2">2. Close extra browser tabs</string>
    <string name="detail_memory_solution_3">3. Reboot the device</string>


    <!--Reboot Recency-->
    <string name="detail_reboot_title_good">Your device has been rebooted recently.</string>
    <string name="detail_reboot_description">Devices should be rebooted weekly to avoid stability and performance issues.</string>

    <string name="detail_reboot_title_bad">Your device hasn\'t been rebooted recently.</string>
    <string name="detail_reboot_title_sub_bad">Devices should be rebooted weekly to avoid stability and performance issues.</string>
    <string name="detail_reboot_solution_1">1. <b>Reboot</b> the device</string>

    <!--Battery-->
    <string name="detail_battery_title_good">Your device has sufficient battery charge.</string>
    <string name="detail_battery_description">Maintaining a good battery charge will prevent the system from automatically slowing down or disabling services.</string>

    <string name="detail_battery_title_bad">Your device has insufficient battery charge.</string>
    <string name="detail_battery_title_sub_bad">Maintaining a good battery charge will prevent the system from automatically slowing down or disabling services.</string>
    <string name="detail_battery_solution_1">1. <b>Charge the device</b> until the battery has at least 40% charge.</string>

    <!--Hardware Performance-->
    <string name="detail_hardware_title_good">Your device\'s hardware is performing well.</string>
    <string name="detail_hardware_description">You\'re running one of the fastest phones available. You shouldn\'t need to upgrade for quite a while, so if your phone is acting slow it may be a symptom of a serious issue.</string>

    <string name="detail_hardware_title_bad">Your device\'s hardware is not performing well.</string>
    <string name="detail_hardware_title_sub_bad">You appear to be using an older or less powerful phone. This may not be of any concern for normal usage. However, if you feel that your phone is too slow you may consider upgrading to a newer or more powerful model. \nReasons you may want a more powerful phone:</string>
    <string name="detail_hardware_solution_1">1. Playing 3D games may run better on newer devices</string>
    <string name="detail_hardware_solution_2">2. Opening many apps or browser tabs at a time can be slow on older devices</string>
    <string name="detail_hardware_solution_3">3. Very large documents may be faster to view and edit on a newer device</string>
    <string name="detail_hardware_solution_4">4. HD video can play smoother on newer CPUs</string>

